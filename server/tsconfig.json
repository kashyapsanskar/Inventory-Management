{
  "compilerOptions": {
    "types": ["node", "@prisma/client","express"],
    "target": "es2016",
    "module": "nodenext",
    "moduleResolution": "nodenext",
    "resolveJsonModule": true,
    "outDir": "./dist",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  },
  "include": [
    "./server/**/*.ts",
    "node_modules/@prisma/client/index.d.ts"
  ]
}
